{
  "GoogleSearch": {
    "pattern": "^(g|goog|google)\\s(.*)$",
    "targetTemplate": "https://google.com/search?q=$1",
    "description": "Redirects to Google search with the provided query."
  },
  "FacebookSearch": {
    "pattern": "^(fb)\\s(.*)$",
    "targetTemplate": "https://www.facebook.com/search/top/?q=$1",
    "description": "Redirects to Facebook search."
  },
  "Telegram": {
    "pattern": "^(tel)$",
    "targetTemplate": "https://web.telegram.org/",
    "description": "Opens Telegram Web."
  },
  "NimaDev": {
    "pattern": "^(nima)$",
    "targetTemplate": "https://nima-dev.com/",
    "description": "Opens Nima's personal website."
  },
  "Gmail": {
    "pattern": "^(gm)$",
    "targetTemplate": "https://mail.google.com/",
    "description": "Opens Gmail."
  },
  "GoogleDrive": {
    "pattern": "^(gd)$",
    "targetTemplate": "https://drive.google.com/",
    "description": "Opens Google Drive."
  },
  "YouTubeSearch": {
    "pattern": "^(yt)\\s(.*)$",
    "targetTemplate": "https://www.youtube.com/results?search_query=$1",
    "description": "Redirects to YouTube search."
  },
  "YouTubeLanding": {
    "pattern": "^(yt)$",
    "targetTemplate": "https://www.youtube.com/",
    "description": "Opens YouTube main page."
  },
  "RedditSearch": {
    "pattern": "^(r)\\s(.*)$",
    "targetTemplate": "https://www.reddit.com/search/?q=$1",
    "description": "Redirects to Reddit search."
  },
  "RedditLanding": {
    "pattern": "^(r)$",
    "targetTemplate": "https://www.reddit.com/",
    "description": "Opens Reddit main page."
  },
  "TwitterSearch": {
    "pattern": "^(tw)\\s(.*)$",
    "targetTemplate": "https://twitter.com/search?q=$1",
    "description": "Redirects to Twitter search."
  },
  "TwitterLanding": {
    "pattern": "^(tw)$",
    "targetTemplate": "https://twitter.com/",
    "description": "Opens Twitter main page."
  },
  "Netflix": {
    "pattern": "^(n)$",
    "targetTemplate": "https://www.netflix.com/",
    "description": "Opens Netflix."
  },
  "AmazonPrimeVideo": {
    "pattern": "^(pv)$",
    "targetTemplate": "https://www.primevideo.com/",
    "description": "Opens Amazon Prime Video."
  },
  "VSCode": {
    "pattern": "^(vs)$",
    "targetTemplate": "vscode://",
    "description": "Opens Visual Studio Code."
  },
  "GoogleCalendar": {
    "pattern": "^(cal)$",
    "targetTemplate": "https://calendar.google.com/",
    "description": "Opens Google Calendar."
  },
  "GitHubDirect": {
    "pattern": "^(gh|github)\\s([\\w-]+)\\/([\\w-]+)$",
    "targetTemplate": "https://github.com/$1/$2",
    "description": "Navigates directly to a GitHub repository."
  },  
  "GitHubSearch": {
    "pattern": "^(gh|github)\\s(.*)$",
    "targetTemplate": "https://github.com/search?q=$1",
    "description": "Redirects to GitHub search."
  },
  "GitHubLanding": {
    "pattern": "^(gh|github)$",
    "targetTemplate": "https://github.com/",
    "description": "Opens GitHub main page."
  },
  "Wealthsimple": {
    "pattern": "^ws$",
    "targetTemplate": "https://www.wealthsimple.com/",
    "description": "Opens Wealthsimple main page."
  },
  "MetaStock": {
    "pattern": "^\\$$",
    "targetTemplate": "https://www.google.com/finance/quote/META:NASDAQ",
    "description": "Opens Meta's stock price."
  },
  "DEFAULT": {
    "pattern": "^()(.*)$",
    "targetTemplate": "https://google.com/search?q=$1",
    "description": "Uses Google search as the default search engine."
  }
}
